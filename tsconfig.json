{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@utils": [
        "src/utils.js"
      ]
    },
    "allowJs": true
  }
}